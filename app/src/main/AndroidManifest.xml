        <activity
            android:name=".LineRootActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".LineYourselfActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".LineParabolaYourselfActivity"
            android:exported="true">
