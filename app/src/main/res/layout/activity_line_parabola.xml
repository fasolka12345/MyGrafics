<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.example.mygrafics.ParabolaYourself
            android:id="@+id/parabolaYourselfView"
            android:layout_width="wrap_content"
            android:layout_height="766dp" />

        <TextView
            android:id="@+id/textCompareParabola"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="casual"
            android:gravity="center"
            android:text="Compare graphes"
            android:onClick="clickCompareParabola"
            android:textColor="#959494"
            android:textSize="34sp" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>